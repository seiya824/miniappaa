<main role="main" class="container">
  <div class="starter-template">
    <div class="contents">
      <div class="contents-tweet">
        <% @tweets.each do |tweet| %>
          <%= render partial: "tweet", locals: {tweet: tweet} %>
        <% end %>
        <div class="page">
          <%= paginate(@tweets) %>
        </div>
      </div>
    </div>
  </div>
</main>
